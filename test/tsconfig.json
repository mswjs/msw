  "compilerOptions": {
    "target": "ES2015",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "declaration": false,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "msw": [".."],
      "msw/*": ["../*"]
    },
    "types": ["node", "jest"]
  },
  "include": ["**/*.ts"]
}
